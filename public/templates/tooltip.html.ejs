<h1 class="<%= Diction.authors.get(author).cssClass() %>">
  <%= Diction.authors.get(author).get('fullname') %>
</h1>
<h2 class="tipsy-count">
  Word Count: <%= count %>
</h2>
<h3 class="tipsy-title">
  <a target="_blank" href="http://www.unhcr.org/<%= documentId %>.html"><%= title %></a>
</h3>
<h3 class="tipsy-date">
  <%= new Date(+date) %>
</h3>
<p class="tipsy-show">
  <a>Show Sentences</a>
</p>


